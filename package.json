{
  "name": "multi-agent-coder",
  "displayName": "Multi-Agent Coder",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "type": "commonjs",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "multi-agent-coder.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "multi-agent-coder.explainCode",
        "title": "Explain Code"
      },
      {
        "command": "multi-agent-coder.viewExplanation",
        "title": "View Saved Explanation"
      }
      
    ],
    "menus": {
      "editor/context": [
        {
          "command": "multi-agent-coder.explainCode",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "explanationHistoryView",
          "name": "Code Explanation History"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/node-fetch": "^2.5.12",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  }
}
